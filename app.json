{
  "expo": {
    "name": "Aura Break",
    "slug": "aurabreak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0E1A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0A0E1A"
      },
      "package": "com.kmkcodelab.aurabreak",
      "permissions": [
        "SYSTEM_ALERT_WINDOW",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-font",
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24,
            "kotlinVersion": "1.9.24",
            "extraProperties": {
        "react.composeCompiler.suppressKotlinVersionCompatibilityCheck": "true"
            }
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8386795368168436~9681094959"
        }
      ],
      "./plugins/withOverlayPermission.js",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "5f8d55a3-3015-456a-babb-5c7e283027ab"
      }
    },
    "owner": "kmkcodelab"
  }
}
